{
  "name": "@novulink/main",
  "version": "0.0.0",
  "engines": {
    "node": ">=22.15.0"
  },
  "packageManager": "npm@11.4.0",
  "scripts": {
    "dev": "vite dev --port 8001",
    "dev-http": "vite dev --port 8001 --https false",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "preview:new": "npm run build && run-p preview",
    "test:unit:coverage": "vitest  --environment jsdom --root src/ run --coverage",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:update": "vitest --environment jsdom --root src/ --update",
    "test:e2e": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test preview https://localhost:4173/ 'cypress run --e2e --config video=false,screenshotOnRunFailure=false'",
    "test:e2e:dev": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'vite dev --port 4173 --host' https://localhost:4173/ 'cypress open --e2e'",
    "test:visual:dev:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'npm run test:visual:dev' http://localhost:3001 'npm run test:visual --ci'",
    "test:visual:ci": "npm run build-storybook  --quiet && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'http-server ./storybook-static -p 3001 --silent' http://localhost:3001 'npm run test:visual --ci'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "translate": "vue-translation-manager translate",
    "translation-key-check": "vue-translation-manager validate",
    "test:all": "run-p coverage test:e2e test:visual:ci",
    "test:visual:dev": "storybook dev -p 3001",
    "build-storybook": "storybook build",
    "test:visual": "test-storybook --url http://localhost:3001 --maxWorkers=2",
    "test:visual:update": "test-storybook --url http://localhost:3001 --updateSnapshot --maxWorkers=2"
  },
  "dependencies": {
    "@m-media/vue3-gate-keeper": "^0.1.5",
    "@m-media/vue3-meta-tags": "^0.0.7",
    "@novulink/api": "*",
    "@novulink/vue-composables": "*",
    "@picocss/pico": "^2.1.1",
    "@stripe/stripe-js": "^7.3.0",
    "@tanstack/vue-query": "^5.76.0",
    "chart.js": "^4.4.9",
    "pinia": "^3.0.2",
    "qr-code-styling": "^1.9.2",
    "type-safe-event-bus": "^1.0.0",
    "vue": "^3.5",
    "vue-chartjs": "^5.3.2",
    "vue-gtag": "^3.5.0",
    "vue-hotjar-next": "^2.1.0",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@novulink/eslint-config": "*",
    "@novulink/helpers": "*",
    "@novulink/types": "*",
    "@pinia/testing": "^1.0.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@storybook/web-components": "^8.6.14",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.19",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-istanbul": "^3.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "acorn-walk": "^8.3.4",
    "axe-playwright": "^2.1.0",
    "cypress": "^14.3.3",
    "eslint": "^9.27.0",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "html-validate": "^9.5.3",
    "html-validate-vue": "^7.1.6",
    "http-server": "^14.1.1",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^26.1.0",
    "lit": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-dark-theme-class": "^1.3.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.12",
    "storybook": "^8.6.14",
    "storybook-addon-mock": "^5.0.0",
    "storybook-vue3-router": "^5.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3",
    "vitest": "^3.1.4",
    "vue-stripe-js": "^2.0.2",
    "vue-translation-manager": "^1.2.0",
    "vue-tsc": "^2.2"
  },
  "pre-commit": [
    "lint",
    "type-check"
  ],
  "pre-push": [
    "translation-key-check",
    "test:unit:coverage",
    "test:visual:ci"
  ]
}
