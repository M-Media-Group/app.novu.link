{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "outputs": [
                "dist/**"
            ]
        },
        "type-check": {
            "dependsOn": [
                "^type-check"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "test:unit:coverage": {
            "dependsOn": [
                "^test:unit:coverage"
            ]
        }
    },
    "extends": [
        "//"
    ]
}